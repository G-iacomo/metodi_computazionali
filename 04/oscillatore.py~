import numpy as np
import scipy
from scipy import integrate
import matplotlib.pyplot as plt
import pandas as pd

x=np.arange(0.1,+5.02,0.1)
y=np.zeros(len(x))
def t(i0):
    for i in range(len(x)):
        if i!=i0:
            y[i]=1/((x[i0]**6-x[i]**6)**0.5)
            else y[i0]=0
            
    t=(8**0.5)*scipy.integrate.simpson(y,x[0:i0])
    return t
T=np.zeros(len(x))
for i in range(len(x)):
    T[i]=t(i)
plt.plot(T,x)
plt.show()
